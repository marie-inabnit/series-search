    <title>Login</title>
</head>
<body>
    <div class="contenu">
        <div class="loginTitle">
            <h4>Login</h4>
        </div>
       
       <div class="inputArea">
           <form action="/users/log-in" method="POST">
               <div class="textArea">
                <i class="fas fa-at"></i>
                    <input type="text" name="emailFront" id="">
               </div>
               <div class="textArea">
                <i class="fas fa-lock"></i>
                    <input type="password" name="passwordFront" id="">
               </div>
               <div class="buttonSubmit">
                    <button type="submit">Enter</button>
               </div>
            </form>
            <div class="registered">
                <a href="/signup-register">Not registered? <i class="fas fa-arrow-circle-right"></i></a>
            </div>

       </div>
    </div>  
   














